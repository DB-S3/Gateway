{
  "Routes": [

    //Basket API
    {
      "DownstreamPathTemplate": "/login/login/{userName}/{password}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "5001"
        }
      ],
      "UpstreamPathTemplate": "/login/{userName}/{password}",
      "UpstreamHttpMethod": [ "GET", "DELETE" ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
    {
      "DownstreamPathTemplate": "/register/register/{userName}/{password}/{mail}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "5001"
        }
      ],
      "UpstreamPathTemplate": "/register/{userName}/{password}/{mail}",
      "UpstreamHttpMethod": [ "GET", "DELETE" ],
      "DangerousAcceptAnyServerCertificateValidator": true
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:7000"
  }
}