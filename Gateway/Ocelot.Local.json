{
  "Routes": [
    {
      "DownstreamPathTemplate": "/page/AddPage/{name}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "ortisy.azurewebsites.net/",
          "port": "443"
        }
      ],
      "UpstreamPathTemplate": "/api/AddPage/{name}",
      "UpstreamHttpMethod": [ "GET", "DELETE" ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
    {
      "DownstreamPathTemplate": "/page/GetPageList",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "ortisy.azurewebsites.net/",
          "port": "443"
        }
      ],
      "UpstreamPathTemplate": "/api/GetPageList",
      "UpstreamHttpMethod": [ "GET", "DELETE" ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
    {
      "DownstreamPathTemplate": "/page/RenamePage/{name}/{pageId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "ortisy.azurewebsites.net/",
          "port": "443"
        }
      ],
      "UpstreamPathTemplate": "/api/RenamePage/{name}/{pageId}",
      "UpstreamHttpMethod": [ "GET", "DELETE" ],
      "DangerousAcceptAnyServerCertificateValidator": true
    },
    {
      "DownstreamPathTemplate": "/page/viewpage/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
